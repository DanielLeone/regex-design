{
    "project": {
        "version": "1.0.0",
        "name": "regex-design"
    },
    "apps": [
        {
            "root": "src",
            "outDir": "dist",
            "assets": [
                "assets",
                "favicon.ico"
            ],
            "index": "index.html",
            "main": "main.ts",
            "test": "test.ts",
            "tsconfig": "tsconfig.json",
            "prefix": "app",
            "mobile": false,
            "styles": [
                "styles.css",
                "normalize.css"
            ],
            "scripts": [
                "DragDropTouch.js",
                "google-analytics.js",
                "../node_modules/mark.js/dist/mark.min.js",
                "../node_modules/clipboard/dist/clipboard.min.js"
            ],
            "environments": {
                "source": "environments/environment.ts",
                "dev": "environments/environment.ts",
                "prod": "environments/environment.prod.ts"
            }
        }
    ],
    "addons": [],
    "packages": [],
    "e2e": {
        "protractor": {
            "config": "./protractor.conf.js"
        }
    },
    "test": {
        "karma": {
            "config": "./karma.conf.js"
        }
    },
    "defaults": {
        "styleExt": "styl",
        "prefixInterfaces": false,
        "inline": {
            "style": true,
            "template": true
        },
        "spec": {
            "class": true,
            "component": true,
            "directive": true,
            "module": true,
            "pipe": true,
            "service": true
        }
    }
}
